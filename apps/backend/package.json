{
  "name": "life-world-os-backend",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "seed:local": "tsx src/scripts/seedWithEnv.ts local",
    "seed:dev": "tsx src/scripts/seedWithEnv.ts dev",
    "seed:local-sample": "tsx src/scripts/seedLocalSample.ts",
    "seed:staging": "tsx src/scripts/seedWithEnv.ts staging",
    "seed:prod": "tsx src/scripts/seedWithEnv.ts prod",
    "migrate:nodes": "tsx src/scripts/migrateRealityNodes.ts",
    "migrate:nodes:dry": "tsx src/scripts/migrateRealityNodes.ts --dry-run",
    "sync:nodes": "tsx src/scripts/syncRealityNodes.ts",
    "sync:nodes:dry": "tsx src/scripts/syncRealityNodes.ts --dry-run",
    "sync:nodes:check": "tsx src/scripts/syncRealityNodes.ts --check",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.37.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.2.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}

